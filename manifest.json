{
  "name": "Distraction Destruction",
  "version": "69.420",
  "description": "Eliminate distractions with time logging and redirects.",
  "permissions": ["tabs", "activeTab", "declarativeContent", "storage", {
    "fileSystem": ["write"]
  }],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "options_page": "options.html",
  "background": {
    "scripts": ["utils.js", "background.js"],
    "persistent": false

  },

  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Toggle feature foo"
    },

    "_execute_page_action": {
      "suggested_key": {
        "windows": "Alt+Shift+P"
      },
      "description": "Page action"
    }
  },

  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
